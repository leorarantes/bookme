{
  "name": "dataapi",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "build": "npx prisma generate && npx prisma db seed && rm -rf ./dist && tsc",
    "start": "node dist/src/index.js",
    "dev": "dotenv -e .env.dev nodemon src/index.ts",
    "migrate:dev": "dotenv -e .env.dev npx prisma migrate dev",
    "migrate:test": "dotenv -e .env.test npx prisma migrate dev",
    "generate:dev": "dotenv -e .env.dev npx prisma generate",
    "seed:dev": "dotenv -e .env.dev npx prisma db seed",
    "pstudio:dev": "dotenv -e .env.dev npx prisma studio",
    "pstudio:test": "dotenv -e .env.test npx prisma studio",
    "test": "rm -rf ./dist && dotenv -e .env.test cross-env NODE_OPTIONS=--experimental-vm-modules jest -i",
    "test:coverage": "dotenv -e .env.test cross-env NODE_OPTIONS=--experimental-vm-modules jest -- -i --coverage --testPathPattern=tests/unit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "joi": "^17.9.2",
    "pg": "^8.11.2",
    "prisma": "^5.1.1",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/joi": "^17.2.3",
    "@types/node": "^20.4.10",
    "@types/pg": "^8.10.2",
    "@types/winston": "^2.4.4",
    "dotenv-cli": "^7.2.1",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
